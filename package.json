{
  "name": "amplify-vite-react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "npm run dev -- --mode test --host=localhost --port=3001 --open",
    "build": "tsc -b && vite build",
    "build:prod": "vite build",
    "build:test": "vite build --mode test",
    "preview": "vite preview --port 3001 --open",
    "lint": "eslint .",
    "lint:write": "eslint --fix .",
    "prettier": "prettier --check .",
    "prettier:write": "prettier --write .",
    "prepare": "([ -d .git ] || [ -d ../.git ]) && npx husky install frontend/.husky || true"
